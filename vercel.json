{
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api/server.js"
		}
	],
	"builds": [
		{
			"src": "api/server.js",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["api/server/**"]
			}
		}
	],
	"functions": {
		"api/server.js": {
			"memory": 1024,
			"maxDuration": 10
		}
	}
}
